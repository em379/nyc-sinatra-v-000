<h1>Edit <%= @landmark.name %></h1>
<form action='/landmarks/<%= @landmark.id %>' method='POST'>

  <input type="hidden" name="_method" value="PATCH">

  <h4>Landmark Name: <input type="text" name="landmark[name]" value="<%= @landmark.name %>" id="name"></h4>

  <h4>Year Completed: <input type="text" name="landmark[year_completed]" value="<%= @landmark.year_completed %>" id="year_completed"></h4>

  <input type="submit" value="Edit Landmark">
</form>
